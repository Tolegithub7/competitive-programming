/**
 * @param {number} n
 * @return {Function} counter
 */
// var createCounter = (n) =>{
//     return () => n++;
// };
var createCounter = function(n) {
    var i = n-1
    return function() {
        return i += 1;
    };
};
/** 
 * const counter = createCounter(10)
 * counter() // 10
 * counter() // 11
 * counter() // 12
 */
