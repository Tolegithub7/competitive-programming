class Solution:
    def majorityElement(self, nums: List[int]) -> int:
        # mycount = Counter(nums)
        # max = 0
        # max_elt = None
        # for elm, count in mycount.items():
        #     if count > max:
        #         max = count
        #         max_elt = elm
        # return max_elt
        nums.sort()
        n = len(nums)
        return(nums[n//2])
