/**
 * @param {Function[]} functions
 * @return {Function}
 */
var compose = function (functions = []) {
    return function (x) {
        if (!functions.length) { return x }
        for (let index = functions.length - 1; index >= 0; index--) {
            x = functions[index](x)
        }
        return x
    }
};

/**
 * const fn = compose([x => x + 1, x => 2 * x])
 * fn(4) // 9
 */
